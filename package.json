{
  "name": "ven-ten",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && babel src -s -d dist --copy-files",
    "dev": "concurrently -k \"npm run start:dev\"",
    "start": "npm run build && node dist/index.js",
    "start:dev": "nodemon --inspect=0.0.0.0:9229 --exec babel-node src/index.js",
    "heroku-postbuild": "npm run dropDB && npm run createDB && npm run reverse && npm run migration && npm run seed && npm run loadCSV",
    "lint": "eslint src/ --fix . && echo 'Lint complete.'",
    "migration": "sequelize db:migrate",
    "dropDB": "node --require @babel/register src/dbConnection/dropDB.js",
    "createDB": "node --require @babel/register src/dbConnection/createDB.js",
    "loadCSV": "node --require @babel/register src/dbConnection/loadCSV.js",
    "migration:test": "cross-env NODE_ENV=test npm run migration",
    "createcar": "sequelize model:create --force --name carowners --attributes first_name:string,last_name:string,email:string,country:string,car_model:string,car_model_year:string,car_color:string,gender:string,job_title:string,bio:text",
    "seed": "sequelize db:seed:all",
    "reverse": "sequelize db:migrate:undo:all",
    "reverse:recent": "sequelize db:migrate:undo",
    "reverse:test": "cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "refreshdb": "npm run dropDB && npm run createDB && npm run reverse && npm run migration && npm run seed",
    "refreshdb:test": "cross-env NODE_ENV=test npm run refreshdb",
    "test": "npm run refreshdb:test && cross-env NODE_ENV=test nyc --require @babel/register ---reporter=html --reporter=text mocha src/test/**.test.js --timeout 20000 --exit"
  },
  "author": "Chima Ekeneme",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/register": "^7.8.6",
    "body-parser": "^1.18.3",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.16.3",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "uuidv4": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.5.5",
    "@babel/node": "~7.5.5",
    "@babel/preset-env": "~7.5.5",
    "chai": "~4.2.0",
    "chai-http": "~4.3.0",
    "concurrently": "~4.1.2",
    "eslint": "~6.2.1",
    "eslint-config-airbnb-base": "~14.0.0",
    "eslint-config-prettier": "~6.1.0",
    "eslint-plugin-import": "~2.18.2",
    "eslint-plugin-prettier": "~3.1.0",
    "expect.js": "~0.3.1",
    "mocha": "~6.2.0",
    "nodemon": "~1.19.1",
    "nyc": "~14.1.1",
    "prettier": "~1.18.2"
  },
  "engines": {
    "node": "12.6.x"
  }
}
